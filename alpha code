#include<stdio.h>
int main()
{
    int t,n,i,j;
    int A[1000];
    long long int c;
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        scanf("%d",&n);
        scanf("%lli",&c);
        for(j=0;j<n;j++)
            {
            scanf("%d",&A[j]);
            c=c-A[j];
            }
                if(c<0)
            {
                    printf("No");
            }
                else
                   {
                    printf("Yes");
                    }
     }
    return 0;
}
